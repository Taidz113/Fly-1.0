-- Biến để kiểm tra trạng thái bay
local isFlying = false
local flySpeed = 50

-- Hàm để bật/tắt bay
local function toggleFly()
    isFlying = not isFlying
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('arwObIsrTvfHDynBJCGVxFfAhTBwztzJNSORiHJMfDdoXSAZpchZdftSHVtYW5vaWQ='))
    local hrp = character:WaitForChild(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('YahMNQZlUSrRdUogjKTbRFyFxfwGuxaBwOXFTALZKvzTaZlUpkbTmRGSHVtYW5vaWRSb290UGFydA=='))
    
    if isFlying then
        ToggleButton.Text = qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('xIkkvuIcnzrwNVEGWSPslJbGNhDZCAkbaIEHTcGlmwJaSFyeZErCowJVOG6r3QgQmF5')
        ToggleButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
        
        -- Tạo BodyVelocity để bay
        local bv = Instance.new(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('kCyDipEAwumpXowXTevaKZEUvpIENanziGbVVsrWPabApVtLKRfVRbfQm9keVZlbG9jaXR5'))
        bv.Velocity = Vector3.new(0, 0, 0)
        bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bv.Parent = hrp
        
        -- Tạo BodyGyro để giữ hướng
        local bg = Instance.new(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('yOjUdssdkproSRYXLZNaNYWptSgXzIciPcGluqlCgKDwpJcatDsUTgCQm9keUd5cm8='))
        bg.P = 9e4
        bg.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        bg.CFrame = hrp.CFrame
        bg.Parent = hrp
        
        -- Vòng lặp để điều khiển bay
        spawn(function()
            while isFlying and character.Parent
            while isFlying and character.Parent do
                local newBV = Vector3.new(0, 0, 0)
                if game:GetService(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('ilZQJwHwcUfylhjtcNptlxjRnVBdxVfLTUSSjBsBHorUoQhoXCPUspFVXNlcklucHV0U2VydmljZQ==')):IsKeyDown(Enum.KeyCode.W) then
                    newBV = newBV + (workspace.CurrentCamera.CoordinateFrame.LookVector * flySpeed)
                end
                if game:GetService(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('IkOhdLNJuikkxQXQpyrRUFMzeuQPMcKBQxycDrLrqHyuHjsVeEgMIICVXNlcklucHV0U2VydmljZQ==')):IsKeyDown(Enum.KeyCode.S) then
                    newBV = newBV - (workspace.CurrentCamera.CoordinateFrame.LookVector * flySpeed)
                end
                if game:GetService(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('gKXApdpSebEseTfRmFsyeqmYXirCrRFlhiAyIXQJiSLglNXRPrUnpzBVXNlcklucHV0U2VydmljZQ==')):IsKeyDown(Enum.KeyCode.A) then
                    newBV = newBV - (workspace.CurrentCamera.CoordinateFrame.RightVector * flySpeed)
                end
                if game:GetService(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('NoLnstDvfzrgiAPMUniiTENgFCjVRDMheBZcjpPprHwjTBlhUnXWGbcVXNlcklucHV0U2VydmljZQ==')):IsKeyDown(Enum.KeyCode.D) then
                    newBV = newBV + (workspace.CurrentCamera.CoordinateFrame.RightVector * flySpeed)
                end
                bv.Velocity = newBV
                bg.CFrame = workspace.CurrentCamera.CoordinateFrame
                wait()
            end
        end)
    else
        ToggleButton.Text = qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('fHhIBkyUXhNckdsrPUreaisdJVyJAVhNvQWRYDevpUCWSOMZmgThHeyQuG6rXQgQmF5')
        ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        
        -- Xóa BodyVelocity và BodyGyro
        for _, v in pairs(hrp:GetChildren()) do
            if v:IsA(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('eiuxPYhdQYTVnsRavPNwczOcLEIswSfBvaZquBWcXSULpuwmidoIvgtQm9keVZlbG9jaXR5')) or v:IsA(qiQDjbCXRIEwDoPflXsSrMfyCTHeeKIxYSlYzgahct('zfKxWHEusTTIBGTiJWpXFovLvGgLaqpUBxoAJTeUOnrBdMtafVXVYTSQm9keUd5cm8=')) then
                v:Destroy()
            end
        end
    end
end

-- Kết nối nút với hàm toggleFly
ToggleButton.MouseButton1Click:Connect(toggleFly)

-- Hàm để chuyển màu tiêu đề
local function changeColor()
    local colors = {
        Color3.fromRGB(255, 0, 0),    -- Đỏ
        Color3.fromRGB(255, 127, 0),  -- Cam
        Color3.fromRGB(255, 255, 0),  -- Vàng
        Color3.fromRGB(0, 255, 0),    -- Lục
        Color3.fromRGB(0, 0, 255),    -- Lam
        Color3.fromRGB(75, 0, 130),   -- Chàm
        Color3.fromRGB(143, 0, 255),  -- Tím
        Color3.fromRGB(255, 192, 203),-- Hồng
        Color3.fromRGB(255, 255, 255),-- Trắng
        Color3.fromRGB(0, 0, 0)       -- Đen
    }
    
    local index = 1
    while true do
        Title.TextColor3 = colors[index]
        index = (index % #colors) + 1
        wait(1)
    end
end

-- Bắt đầu chuyển màu
spawn(changeColor)    
